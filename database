values ('3','perfume','9' ,'3','1');

insert into  warehouse (Item_number, Item_description ,shelve_number,Quantity_in_stock, price) 
values ('4','microwave','5' ,'4','4');

insert into  warehouse (Item_number, Item_description ,shelve_number,Quantity_in_stock, price) 
values ('5','bed','2' ,'5','5');


select*from Warehouse




delete Warehouse

